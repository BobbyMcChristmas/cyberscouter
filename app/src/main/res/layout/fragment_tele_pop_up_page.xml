<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textAlignment="center"
    tools:context=".TelePopUpPage">

    <TextView
        android:id="@+id/LowerBox"
        android:layout_width="134dp"
        android:layout_height="48dp"
        android:layout_marginTop="64dp"
        android:text="@string/lower_text"
        android:textAlignment="center"
        android:textColor="#020000"
        android:textSize="40sp"
        app:layout_constraintTop_toBottomOf="@+id/outerScore"
        tools:layout_editor_absoluteX="458dp" />

    <Button
        android:id="@+id/LowerMinusButton"
        android:layout_width="54dp"
        android:layout_height="53dp"
        android:layout_marginTop="8dp"
        android:text="@string/MinusButton"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="@+id/outerMinusButton"
        app:layout_constraintTop_toBottomOf="@+id/LowerBox" />

    <EditText
        android:id="@+id/lowerScore"
        android:layout_width="56dp"
        android:layout_height="57dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/ZeroText"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/outerScore"
        app:layout_constraintTop_toBottomOf="@+id/LowerBox" />

    <Button
        android:id="@+id/lowerPlusButton"
        android:layout_width="52dp"
        android:layout_height="52dp"
        android:layout_marginTop="8dp"
        android:text="@string/PlusButton"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="@+id/outerPlusButton"
        app:layout_constraintTop_toBottomOf="@+id/LowerBox" />

    <TextView
        android:id="@+id/OuterBox"
        android:layout_width="134dp"
        android:layout_height="48dp"
        android:layout_marginTop="64dp"
        android:text="@string/outer_text"
        android:textAlignment="center"
        android:textColor="#020000"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="@+id/LowerBox"
        app:layout_constraintStart_toStartOf="@+id/LowerBox"
        app:layout_constraintTop_toBottomOf="@+id/InnerScore" />

    <Button
        android:id="@+id/outerPlusButton"
        android:layout_width="52dp"
        android:layout_height="52dp"
        android:layout_marginTop="8dp"
        android:text="@string/PlusButton"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="@+id/InnerPlusButton"
        app:layout_constraintTop_toBottomOf="@+id/OuterBox" />

    <EditText
        android:id="@+id/outerScore"
        android:layout_width="56dp"
        android:layout_height="57dp"
        android:layout_marginTop="8dp"
        android:autofillHints=""
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/ZeroText"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/InnerScore"
        app:layout_constraintTop_toBottomOf="@+id/OuterBox" />

    <Button
        android:id="@+id/outerMinusButton"
        android:layout_width="54dp"
        android:layout_height="53dp"
        android:layout_marginTop="8dp"
        android:text="@string/MinusButton"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="@+id/InnerMinusButton"
        app:layout_constraintTop_toBottomOf="@+id/OuterBox" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="384dp"
        android:layout_height="820dp"
        android:src="@drawable/powerport"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/InnerBox"
        android:layout_width="134dp"
        android:layout_height="48dp"
        android:layout_marginStart="74dp"
        android:layout_marginTop="120dp"
        android:text="@string/inner_text"
        android:textAlignment="center"
        android:textColor="#020000"
        android:textSize="40sp"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/InnerPlusButton"
        android:layout_width="52dp"
        android:layout_height="52dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:text="@string/PlusButton"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/InnerScore"
        app:layout_constraintTop_toBottomOf="@+id/InnerBox" />

    <EditText
        android:id="@+id/InnerScore"
        android:layout_width="56dp"
        android:layout_height="57dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/ZeroText"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/InnerMinusButton"
        app:layout_constraintTop_toBottomOf="@+id/InnerBox" />

    <Button
        android:id="@+id/InnerMinusButton"
        android:layout_width="54dp"
        android:layout_height="53dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="8dp"
        android:text="@string/MinusButton"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/InnerBox" />

    <EditText
        android:id="@+id/textView_Zone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:inputType="text"
        android:text="@string/zone_text"
        android:textColor="@android:color/black"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/BackButton"
        android:layout_width="127dp"
        android:layout_height="67dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="32dp"
        android:backgroundTint="#008F2929"
        android:text="@string/back_text"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</android.support.constraint.ConstraintLayout>