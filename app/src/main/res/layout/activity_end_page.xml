<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EndPage">

    <Button
        android:id="@+id/button_LostCommNo"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="32dp"
        android:text="@string/NoButton"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/button_LostCommYes"
        app:layout_constraintTop_toBottomOf="@+id/textView_RobotProblem" />

    <Button
        android:id="@+id/button_BrokeDownNo"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="32dp"
        android:text="@string/NoButton"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/button_LostCommYes"
        app:layout_constraintTop_toBottomOf="@+id/button_LostCommNo" />

    <Button
        android:id="@+id/button_SubSystemBrokeNo"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="32dp"
        android:text="@string/NoButton"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/button_LostCommYes"
        app:layout_constraintTop_toBottomOf="@+id/button_BrokeDownNo" />

    <Button
        android:id="@+id/button_GroundPickupNo"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="32dp"
        android:text="@string/NoButton"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/button_LostCommYes"
        app:layout_constraintTop_toBottomOf="@+id/button_SubSystemBrokeNo" />

    <Button
        android:id="@+id/button_HopperLoadNo"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="32dp"
        android:text="@string/NoButton"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/button_LostCommYes"
        app:layout_constraintTop_toBottomOf="@+id/button_GroundPickupNo" />

    <Button
        android:id="@+id/button_LostCommYes"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:text="@string/YesButton"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/textView_LostComm"
        app:layout_constraintTop_toBottomOf="@+id/textView_RobotProblem" />

    <Button
        android:id="@+id/button_BrokeDownYes"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:text="@string/YesButton"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/textView_LostComm"
        app:layout_constraintTop_toBottomOf="@+id/button_LostCommYes" />

    <Button
        android:id="@+id/button_SubSystemBrokeYes"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:text="@string/YesButton"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/textView_LostComm"
        app:layout_constraintTop_toBottomOf="@+id/button_BrokeDownYes" />

    <Button
        android:id="@+id/button_GroundPickupYes"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:text="@string/YesButton"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/textView_LostComm"
        app:layout_constraintTop_toBottomOf="@+id/button_SubSystemBrokeYes" />

    <Button
        android:id="@+id/button_HopperLoadYes"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:text="@string/YesButton"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/textView_LostComm"
        app:layout_constraintTop_toBottomOf="@+id/button_GroundPickupYes" />

    <Button
        android:id="@+id/button7"
        android:layout_width="173dp"
        android:layout_height="95dp"
        android:layout_marginBottom="24dp"
        android:text="@string/PreviousButton"
        android:textSize="20sp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.038"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button_previous"
        android:layout_width="226dp"
        android:layout_height="80dp"
        android:layout_marginBottom="16dp"
        android:text="@string/PreviousButton"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.054"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button67"
        android:layout_width="226dp"
        android:layout_height="80dp"
        android:layout_marginBottom="16dp"
        android:text="@string/NextButton"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.972"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView_roleTag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="@string/EndPageText"
        android:textColor="@android:color/black"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_RobotProblem"
        android:layout_width="230dp"
        android:layout_height="56dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="96dp"
        android:text="@string/RobotProblemsText"
        android:textColor="@android:color/black"
        android:textSize="32sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_LostComm"
        android:layout_width="128dp"
        android:layout_height="48dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="32dp"
        android:text="@string/LostCommText"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_RobotProblem" />

    <TextView
        android:id="@+id/textView_BrokeDown"
        android:layout_width="128dp"
        android:layout_height="48dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="32dp"
        android:text="@string/BrokeDownText"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_LostComm" />

    <TextView
        android:id="@+id/textView_SubSystem"
        android:layout_width="196dp"
        android:layout_height="48dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="32dp"
        android:text="@string/SubSystemText"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_BrokeDown" />

    <TextView
        android:id="@+id/textView_GroundPickup"
        android:layout_width="172dp"
        android:layout_height="48dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="32dp"
        android:text="@string/GroundPickupText"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_SubSystem" />

    <TextView
        android:id="@+id/textView6_HopperLoad"
        android:layout_width="144dp"
        android:layout_height="48dp"
        android:layout_marginStart="108dp"
        android:layout_marginTop="32dp"
        android:text="@string/HopperLoadText"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_GroundPickup" />

    <TextView
        android:id="@+id/textView_DefenseText"
        android:layout_width="350dp"
        android:layout_height="36dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="40dp"
        android:layout_marginBottom="128dp"
        android:text="@string/DefenseText"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/button_previous"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6_HopperLoad" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="128dp"
        android:layout_marginTop="16dp"
        android:text="Button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_DefenseText" />

</android.support.constraint.ConstraintLayout>